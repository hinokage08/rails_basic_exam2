<h1>instagram</h1>
<p><%= notice %></p>

<table>
  <tr>
    <th>内容</th>
  </tr>
<% @websites.each do |website| %>
  <tr>
    <td><%= website.content %></td>
    <td><%= link_to '詳細', website_path(website.id) %></td>
    <td><%= link_to '編集', edit_website_path(website.id) %></td>
    <td><%= link_to '削除', website_path(website.id), method: :delete,data: { confirm: '本当に削除していいですか'} %></td>
  </tr>
<% end %>
</table>

<%= link_to '新しく投稿する', new_website_path %>
